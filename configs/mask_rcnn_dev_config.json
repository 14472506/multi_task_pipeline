{
    "model":{
        "model_name": "Mask_RCNN_Resnet_50_FPN",
        "pre_trained": true,
        "loaded": true,
        "load_source": "outputs/All_RGB_SSL/rotnet_pt_1/rotnet_best.pth",
        "trainable_layers": 5,
        "num_classes": 2
    },   
    "dataset":{
        "train":{
            "type": "train",
            "dir": "datasets/sources/jersey_dataset_v4/train",
            "json_dir": "datasets/sources/jersey_dataset_v4/train/train.json",
            "batch_size": 1,
            "shuffle": true,
            "num_workers": 0,
            "augment": false
        },
        "val":{
            "type": "val",
            "dir": "datasets/sources/jersey_dataset_v4/val",
            "json_dir": "datasets/sources/jersey_dataset_v4/val/val.json",
            "batch_size": 1,
            "shuffle": false,
            "num_workers": 0,
            "augment": false

        },
        "test":{
            "type": "test",
            "dir": "datasets/sources/jersey_dataset_v4/test",
            "json_dir": "datasets/sources/jersey_dataset_v4/test/test.json",
            "batch_size": 1,
            "shuffle": false,
            "num_workers": 0,
            "augment": false
        }
    },
    "optimizer":{
        "opt_name": "Adam",
        "opt_lr": 0.00005, 
        "sched_name": "StepLR",
        "sched_step": 50,
        "sched_gamma": 0.1
    },
    "loop":{
        "train": false,
        "test": true,
        "start_epoch": 0, 
        "end_epoch": 80,
        "loop_type": "instance",
        "device": "cuda:0",
        "scheduler": true,
        "amp": true
    },
    "logging":{
        "logger": "base_logger",
        "path": "outputs/All_RGB_SSL/Rotnet_pt_Mask_RCNN",
        "pth_name": "best.pth"

    }
}
